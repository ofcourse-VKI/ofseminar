FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.12;

boundaryField
{
    "(top|diaph|core|acs|vessel|pump)"
    {
        type            omegaWallFunction;
        value           uniform 0.12;
    }
    "symmetry.*"
    {
        type            symmetry;
    }
}



